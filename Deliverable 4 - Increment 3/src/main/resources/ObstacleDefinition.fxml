<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ObstacleDefinitionController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="50.0" layoutY="50.0" text="Obstacle Name:">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="obstacleNameInput" layoutX="150.0" layoutY="45.0" prefWidth="200.0"/>

                <Label layoutX="50.0" layoutY="100.0" text="Obstacle Height (m):">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="obstacleHeightInput" layoutX="150.0" layoutY="95.0" prefWidth="200.0"/>

                <Label layoutX="50.0" layoutY="150.0" text="Obstacle Width (m):">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="obstacleWidthInput" layoutX="150.0" layoutY="145.0" prefWidth="200.0"/>

                <Button fx:id="submitButton" layoutX="150.0" layoutY="200.0" text="Submit" onAction="#handleSubmitButtonClick"/>
                <Button fx:id="backButton" layoutX="350.0" layoutY="200.0" text="Back" onAction="#handleBackButtonClick"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
