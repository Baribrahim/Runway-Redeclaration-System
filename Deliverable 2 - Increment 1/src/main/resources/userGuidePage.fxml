<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="593.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.userGuidePageController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="342.0" layoutY="4.0" style="&#10;" text="Definitions" textAlignment="CENTER" wrapText="false">
          <font>
            <Font name="Janna LT Regular" size="24.0" />
          </font>
        </Label>
            <Button fx:id="goBackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackButtonClick" text="Back" />
            <Label alignment="CENTER" layoutX="12.0" layoutY="70.0" style="&#10;" text="Take-off Run Available (TORA)" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The length of the runway available for take-­off.  This is the total distance from the point where an aircraft can commence&#10; its take-­off to the point where the runway can no longer support the weight of the aircraft under normal conditions.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
            <Label alignment="CENTER" layoutX="12.0" layoutY="137.0" style="&#10;" text="Take-off Distance Available (TODA)" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="170.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The length of the runway (TORA) plus any Clearway. &#10;This is the total distance an aircraft can safely utilise for its take-­off and initial ascent.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
            <Label alignment="CENTER" layoutX="12.0" layoutY="203.0" style="&#10;" text="Accelerate-Stop Distance Available (ASDA)" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="236.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The length of the runway (TORA) plus any Stopway. This is the total distance available to the aircraft in case of an aborted take-­off.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
            <Label alignment="CENTER" layoutX="12.0" layoutY="258.0" style="&#10;" text="Landing Distance Available (ASDA)" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="291.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The length of the runway available for landing. The start of this is called the threshold and is typically the same as the start of the TORA. &#10;A threshold may be displaced for operational reasons or because of a temporary obstacle, in which case LDA and TORA can differ.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
            <Label alignment="CENTER" layoutX="12.0" layoutY="323.0" style="&#10;" text="Approach Landing Surface (ALS)" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="356.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The surface formed between the top of the obstacle and the runway when taking into account the airport’s minimum angle of descent.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
            <Label alignment="CENTER" layoutX="12.0" layoutY="378.0" style="&#10;" text="Take-off Climb Surface (TOCS)" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="411.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The surface formed between the runway and the top of the obstacle when taking into account the airport’s minimum angle of ascent.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
            <Label alignment="CENTER" layoutX="12.0" layoutY="429.0" style="&#10;" text="Runway End Safety Area (RESA)" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="462.0" strokeType="OUTSIDE" strokeWidth="0.0" text="An area symmetrical about the extended runway centreline and adjacent to the end of the strip primarily intended to reduce the risk &#10;of damage to an aircraft undershooting or overrunning the runway.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
            <Label alignment="CENTER" layoutX="12.0" layoutY="495.0" style="&#10;" text="Blast Protection" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Verdana Bold" size="13.0" />
               </font>
            </Label>
            <Text layoutX="12.0" layoutY="528.0" strokeType="OUTSIDE" strokeWidth="0.0" text="A safety area behind an aircraft to prevent the engine blast from affecting any obstacles located behind it.">
               <font>
                  <Font size="11.0" />
               </font>
            </Text>
      </children>
    </AnchorPane>
  </children>
</VBox>
